{
  "name": "generator-reazy",
  "description": "A Yeoman generator for a Reazy application",
  "version": "0.1.3",
  "homepage": "https://github.com/GeekyAnts/generator-reazy",
  "main": "generators/app/index.js",
  "license": "MIT",
  "keywords": [
    "reazy",
    "reazy-plugin",
    "yeoman-generator",
    "yeoman"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/GeekyAnts/generator-reazy.git"
  },
  "author": "Kumar Sanket, Himanshu Satija, Sankhadeep Roy",
  "bugs": {
    "url": "https://github.com/GeekyAnts/generator-reazy/issues"
  },
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "publish": "git push origin && git push origin --tags",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "jshint": "jshint lib/. generators/**/index.js test/. --config",
    "test:transform": "mocha test/transform.test.js --compilers js:babel-core/register",
    "test:generators": "mocha test/generators.test.js --timeout 300000",
    "test": "npm run jshint && npm run test:transform && npm run test:generators"
  },
  "files": [
    "generators/app",
    "generators/service",
    "lib"
  ],
  "dependencies": {
    "ast-traverse": "^0.1.1",
    "chalk": "^1.1.1",
    "cross-spawn-async": "^2.1.9",
    "i": "^0.3.3",
    "object.assign": "^4.0.3",
    "recast": "^0.11.2",
    "repeating": "^2.0.0",
    "string": "^3.3.1",
    "string-length": "^1.0.1",
    "update-notifier": "^1.0.1",
    "yeoman-generator": "^0.24.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "jshint": "^2.9.2",
    "mocha": "^3.0.0",
    "yeoman-test": "^1.4.0"
  }
}
